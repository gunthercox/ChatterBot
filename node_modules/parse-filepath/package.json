{
  "name": "parse-filepath",
  "description": "Pollyfill for node.js `path.parse`, parses a filepath into an object.",
  "version": "1.0.2",
  "homepage": "https://github.com/jonschlinkert/parse-filepath",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/parse-filepath",
  "bugs": {
    "url": "https://github.com/jonschlinkert/parse-filepath/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.8"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "is-absolute": "^1.0.0",
    "map-cache": "^0.2.0",
    "path-root": "^0.1.1"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.7",
    "mocha": "^2.2.5",
    "should": "^7.0.2"
  },
  "keywords": [
    "absolute",
    "basename",
    "dir",
    "directory",
    "dirname",
    "ext",
    "extension",
    "extname",
    "file",
    "filename",
    "filepath",
    "is-absolute",
    "name",
    "object",
    "parse",
    "parser",
    "parts",
    "path",
    "segment"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "highlight": "global-prefix",
      "list": [
        "global-prefix",
        "is-absolute",
        "is-relative",
        "relative"
      ]
    },
    "reflinks": [
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
